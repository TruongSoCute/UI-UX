<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/BtnSearch"
        android:layout_width="138dp"
        android:layout_height="50dp"
        android:drawableStart="@drawable/search"
        android:paddingStart="50dp"
        android:background="@drawable/shape_but"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <Button
        android:id="@+id/BtnHome"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/shape_but"
        android:layout_marginLeft="-2dp"
        android:drawableStart="@drawable/home_logo"
        android:paddingStart="50dp"
        app:layout_constraintLeft_toRightOf="@id/BtnSearch"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <Button
        android:id="@+id/BtnFood"
        android:layout_width="138dp"
        android:layout_height="50dp"
        android:background="@drawable/shape_but"
        android:drawableStart="@drawable/food"
        android:paddingStart="50dp"
        android:layout_marginLeft="-2dp"
        app:layout_constraintLeft_toRightOf="@id/BtnHome"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/BtnTatCa"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/buttonshow"
        android:text="29"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/TvTatCa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:id="@+id/TvTatCa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tất Cả"
        android:textColor="#A9000000"
        android:textStyle="italic"
        android:textSize="20dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/TvHome"
        app:layout_constraintBottom_toTopOf="@id/BtnTatCa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/TvConLai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Còn Lại"
        android:textColor="#A9000000"
        android:textStyle="italic"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/BtnTatCa"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/BtnConLai"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/buttonshow"
        android:text="27"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/TvConLai"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />

    <TextView
        android:id="@+id/TvHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TRANG CHỦ"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="30dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/TvDangThue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Đang Thuê"
        android:textColor="#A9000000"
        android:textStyle="italic"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/BtnConLai"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/BtnDangThue"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/buttonshow_blue1"
        android:text="1"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/white"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/TvDangThue"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:id="@+id/TvDangSua"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Đang Sửa"
        android:textColor="#A9000000"
        android:textStyle="italic"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/BtnDangThue"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/BtnDangSua"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/buttonshow_blue2"
        android:text="1"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/white"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/TvDangSua"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:id="@+id/TvDatPhong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Đặt Phòng"
        android:textColor="#A9000000"
        android:textStyle="italic"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/BtnDangSua"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/BtnDatPhong"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/buttonshow_blue2"
        android:drawableStart="@drawable/baseline_border_color_24"
        android:paddingStart="85dp"
        android:textStyle="bold"
        android:textSize="25dp"
        android:textColor="@color/white"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/TvDatPhong"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />


</androidx.constraintlayout.widget.ConstraintLayout>